{
  "name": "simpleneed",
  "app_id": "",
  "v2": true,
  "typescript": true,
  "gulpStartupTasks": [
    "sass",
    "watch"
  ],
  "watchPatterns": [
    "www/**/*",
    "!www/lib/**/*",
    "!www/**/*.map"
  ],
  "proxies": [
    {
      "path": "/rest/v1/api/",
      "proxyUrl": "http://localhost:8100/rest/v1/api/"
    }
  ]
}
